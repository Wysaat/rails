<%provide(:title, "Sign in") %>
<p>Sign In Here!</p>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(:session, url: sessions_path) do |f|%>
      <%= f.label :email %>
      <%= f.text_field :email %>
      <%= f.label :password %>
      <%= f.password_field :password %>
      <%= f.submit "Sign in",class: "btn btn-large btn-primary" %>
    <% end %>
    <p>I'm new, <%= link_to "Sign Me Up!",signup_path %></p>
  </div>
</div>